{
  "name": "test-chatbot",
  "version": "1.0.0",
  "description": "React application with Express server demonstrating AI SDK data streaming and human-in-the-loop",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "npm run dev",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install --legacy-peer-deps",
    "cleanup": "./cleanup-ports.sh",
    "stop": "./cleanup-ports.sh"
  },
  "keywords": [
    "ai-sdk",
    "streaming",
    "human-in-the-loop",
    "react",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
